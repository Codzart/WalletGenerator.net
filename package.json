{
  "name": "walletgenerator.net",
  "version": "3.3.0",
  "description": "Open Source JavaScript Client-Side Bitcoin Wallet Generator",
  "main": "Gruntfile.js",
  "dependencies": {
    "base58": "^2.0.1",
    "bchaddrjs": "^0.3.0",
    "bip38": "^2.0.2",
    "bitcoinjs-lib": "^3.3.2",
    "bitcoinjs-lib-zcash": "^3.5.3",
    "bn.js": "^4.11.8",
    "browserify-aes": "^1.2.0",
    "elliptic": "^6.4.1",
    "ethereumjs-wallet": "^0.6.3",
    "grunt": "~0.4.1",
    "grunt-combine": "~0.8.3",
    "grunt-lineending": "~0.2.4",
    "lodash": "^4.17.11",
    "minimatch": ">=3.0.2",
    "nem-sdk": "^1.6.7",
    "randombytes": "^2.0.6",
    "ripple-keypairs": "^0.11.0",
    "safe-buffer": "^5.1.2",
    "scryptsy": "^2.0.0",
    "wif": "^2.0.6"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/MichaelMure/WalletGenerator.net.git"
  },
  "keywords": [
    "bitcoin address wallet generator"
  ],
  "scripts": {
    "init": "npm i",
    "start": "npm run export ; grunt",
    "export": "browserify src/init.js -o exported.js ; google-closure-compiler --js=exported.js --jscomp_off=\"*\" --warning_level=QUIET --js_output_file=exported-comp.js ; rm exported.js",
    "public": "npm run start ; rm -rf public ; mkdir public ; cp -R logos/ images/ wallets/ index.html public/",
    "lint": "cd src ; eslint ninja.*.js janin.*.js qrcode.js doge.js init.js misc.js handlers.js coins/ lazy/ ../tests/",
    "pret": "cd src ; prettier --print-width 200 --write ninja.*.js janin.*.js qrcode.js doge.js init.js misc.js handlers.js coins/*.js lazy/*.js ../tests/*.js",
    "browser": "npm run public ; browser-sync start --server --files=./public/**",
    "reload": "npm run public ; browser-sync reload",
    "test": "mocha tests/"
  },
  "author": "MichaelMure",
  "license": "MIT",
  "readmeFilename": "README.md",
  "bugs": {
    "url": "https://github.com/MichaelMure/WalletGenerator.net/issues"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "babel-eslint": "^10.0.1",
    "bitcoinjs-lib": "^3.3.2",
    "browser-sync": "^2.26.3",
    "browserify": "^16.2.3",
    "eslint": "^5.15.0",
    "google-closure-compiler": "^20190215.0.2",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "mocha": "^6.0.2",
    "prettier": "^1.16.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{json,md}": [
      "prettier --print-width 200 --write",
      "git add"
    ],
    "*.js": [
      "prettier --print-width 200 --write",
      "eslint",
      "git add"
    ],
    "*.png": [
      "optipng -strip all -o9",
      "git add"
    ]
  }
}
