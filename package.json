{
  "name": "walletgenerator.net",
  "version": "3.3.0",
  "description": "Open Source JavaScript Client-Side Bitcoin Wallet Generator",
  "main": "Gruntfile.js",
  "dependencies": {
    "base58": "^2.0.1",
    "bchaddrjs": "^0.3.0",
    "bip38": "^2.0.2",
    "bitcoinjs-lib": "^3.3.2",
    "bitcoinjs-lib-zcash": "^3.5.3",
    "bn.js": "^4.11.8",
    "browserify-aes": "^1.2.0",
    "elliptic": "^6.4.1",
    "ethereumjs-wallet": "^0.6.3",
    "nem-sdk": "^1.6.7",
    "randombytes": "^2.0.6",
    "ripple-keypairs": "^0.11.0",
    "safe-buffer": "^5.1.2",
    "scryptsy": "^2.0.0",
    "wif": "^2.0.6"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/MichaelMure/WalletGenerator.net.git"
  },
  "keywords": [
    "bitcoin address wallet generator"
  ],
  "scripts": {
    "init": "npm i",
    "start": "npm run export",
    "export": "node ./util/gen ; rm -rf public ; webpack",
    "public": "npm run start ; cp src/index.html public/",
    "lint": "cd src ; eslint ninja.*.js janin.*.js qrcode.js doge.js init.js misc.js handlers.js coins/ lazy/ lodash/ ../tests/",
    "pret": "cd src ; prettier --print-width 200 --write ninja.*.js janin.*.js qrcode.js doge.js init.js misc.js handlers.js coins/*.js lazy/*.js lodash/*.js ../tests/*.js ../util/*.js",
    "browser": "npm run public ; cd public ; browser-sync start --config ../bs-config.js",
    "reload": "npm run public ; browser-sync reload",
    "test": "mocha tests/"
  },
  "author": "MichaelMure",
  "license": "MIT",
  "readmeFilename": "README.md",
  "bugs": {
    "url": "https://github.com/MichaelMure/WalletGenerator.net/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "assert": "^1.4.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "browser-sync": "^2.26.3",
    "css-loader": "^2.1.1",
    "eslint": "^5.15.0",
    "file-loader": "^3.0.1",
    "google-closure-compiler": "^20190215.0.2",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "mocha": "^6.0.2",
    "prettier": "^1.16.4",
    "style-loader": "^0.23.1",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{json,md}": [
      "prettier --print-width 200 --write",
      "git add"
    ],
    "*.js": [
      "prettier --print-width 200 --write",
      "eslint",
      "git add"
    ],
    "*.png": [
      "optipng -strip all -o9",
      "git add"
    ]
  }
}
