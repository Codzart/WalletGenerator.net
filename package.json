{
  "name": "walletgenerator.net",
  "version": "3.3.0",
  "description": "Open Source JavaScript Client-Side Bitcoin Wallet Generator",
  "main": "Gruntfile.js",
  "dependencies": {
    "base58": "^2.0.1",
    "bchaddrjs": "^0.3.0",
    "bip38": "^2.0.2",
    "bitcoinjs-lib": "^3.3.2",
    "bitcoinjs-lib-zcash": "^3.5.3",
    "bn.js": "^4.11.8",
    "browserify-aes": "^1.2.0",
    "elliptic": "^6.4.1",
    "randombytes": "^2.0.6",
    "safe-buffer": "^5.1.2",
    "scryptsy": "^2.0.0",
    "wif": "^2.0.6",
    "grunt": "~0.4.1",
    "grunt-combine": "~0.8.3",
    "grunt-lineending": "~0.2.4",
    "minimatch": ">=3.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/MichaelMure/WalletGenerator.net.git"
  },
  "keywords": [
    "bitcoin address wallet generator"
  ],
  "scripts": {
    "init": "npm i",
    "start": "npm run export ; grunt",
    "export": "browserify src/exports.js -o exported.js ; google-closure-compiler --js=exported.js --js_output_file=src/exported.js ; rm exported.js",
    "public": "npm run start ; rm -rf public ; mkdir public ; cp -R logos/ images/ wallets/ index.html public/"
  },
  "author": "MichaelMure",
  "license": "MIT",
  "readmeFilename": "README.md",
  "bugs": {
    "url": "https://github.com/MichaelMure/WalletGenerator.net/issues"
  },
  "devDependencies": {
    "bitcoinjs-lib": "^3.3.2",
    "browserify": "^16.2.3",
    "google-closure-compiler": "^20190215.0.2"
  }
}
